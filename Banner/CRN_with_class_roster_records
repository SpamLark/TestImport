SELECT DISTINCT 
    SFRSTCR_CRN,
    SSBSECT_SUBJ_CODE, 
    SSBSECT_CRSE_NUMB, 
    SFRSTCR_TERM_CODE, 
    SFRSTCR_PTRM_CODE, 
    SSBSECT_GMOD_CODE, 
    SPRIDEN_FIRST_NAME, 
    SPRIDEN_LAST_NAME
FROM 
    SFRSTCR
	JOIN SSBSECT ON SFRSTCR_CRN = SSBSECT_CRN AND SFRSTCR_TERM_CODE = SSBSECT_TERM_CODE
    LEFT JOIN SIRASGN ON SFRSTCR_CRN = SIRASGN_CRN AND SFRSTCR_TERM_CODE = SIRASGN_TERM_CODE
    LEFT JOIN SPRIDEN ON SIRASGN_PIDM = SPRIDEN_PIDM
ORDER BY SSBSECT_SUBJ_CODE, SSBSECT_CRSE_NUMB